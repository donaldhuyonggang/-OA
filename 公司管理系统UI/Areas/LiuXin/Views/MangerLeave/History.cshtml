
@{
    ViewBag.Title = "History";
    Layout = "~/Views/Background/BackgroundPage1.cshtml";
}
@using 公司管理系统Model
@model List<Leave>
<link href="~/Content/bootstrap.css" rel="stylesheet" />
<div class="divtablemore">
    <table class="table table-bordered table-striped">

        <tr>
            <th> </th>
            <th>请假人</th>
            <th>联系方式</th>
            <th>请假时间</th>
            <th>返校时间</th>
            <th>请假原因</th>
            <th>经理批准</th>
            <th>老师审查</th>
        </tr>
       @foreach (var item in Model)
       {
               <tr>
                <td><img src="~/Content/images/@item.User.Userimg" style="width:40px; height:40px;" /></td>
                <td>@item.User.UserName</td>
                <td>@item.User.Adderss</td>
                <td>@item.LeaveTime</td>
                <td>@item.LeaveResult</td>
                <td>@item.LeaveType</td>
                <td>@item.LeaveOver</td>
                <td>@item.Leave_Condition</td>
            </tr>
       }
    </table>
</div>